package model;

public class PilhaInt {

	No topo;
	
	public PilhaInt() {
		//topo é a base no início da pilha
		topo = null;
	}
	
	public boolean isEmpty() {
		if(topo == null) {
			return true;
		}
		else if (topo != null) {
			return false;
		}
	}
	
	public void push(int valor) {
		No elemento = new No();
		elemento.dado = valor;
		elemento.proximo = topo;
		topo = elemento;
	}

}
